{
  "name": "@fhevm/sdk",
  "version": "1.0.0",
  "description": "Universal FHEVM SDK for building confidential frontend applications - Framework agnostic wrapper for encrypted computations",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.mjs",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./vue": {
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.js",
      "types": "./dist/vue.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/react.ts src/vue.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/react.ts src/vue.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "privacy",
    "zama",
    "blockchain",
    "ethereum",
    "web3",
    "sdk",
    "react",
    "vue",
    "nextjs",
    "nodejs"
  ],
  "author": "FHEVM Community",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "fhevmjs": "^0.6.2",
    "ethers": "^6.15.0",
    "viem": "^2.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.0",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/fhevm-universal-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/fhevm-universal-sdk/issues"
  },
  "homepage": "https://github.com/your-username/fhevm-universal-sdk#readme"
}
