{
  "name": "fhevm-nodejs-example",
  "version": "1.0.0",
  "description": "Node.js server-side example with Universal FHEVM SDK",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "example:encrypt": "tsx src/examples/encrypt.ts",
    "example:decrypt": "tsx src/examples/decrypt.ts",
    "example:contract": "tsx src/examples/contractInteraction.ts"
  },
  "dependencies": {
    "@fhevm/sdk": "^0.3.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
    "fhevmjs": "^0.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  }
}
